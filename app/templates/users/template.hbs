authenticated: {{session.isAuthenticated}}

<nav id="navigation-bar">
	<!-- Left -->
	<ul>
		<li>{{#link-to 'index'}}Polichess{{/link-to}}</li>
	</ul>
	<!-- Right -->
	<ul>
		<li>{{#link-to 'users'}}Usuarios{{/link-to}}</li>
		{{#if session.isAuthenticated}}
			<li>{{#link-to 'index' invokeAction='invalidateSession'}}Log out{{/link-to}}</li>
			<li>{{#link-to 'users.sign_up'}}Perfil{{/link-to}}</li>
		{{else}}
			<li>{{#link-to 'users.log_in'}}Log in{{/link-to}}</li>
			<li>{{#link-to 'users.sign_up'}}Sign up{{/link-to}}</li>
		{{/if}}
	</ul>
</nav>

<div id="content">
	{{outlet}}
</div>